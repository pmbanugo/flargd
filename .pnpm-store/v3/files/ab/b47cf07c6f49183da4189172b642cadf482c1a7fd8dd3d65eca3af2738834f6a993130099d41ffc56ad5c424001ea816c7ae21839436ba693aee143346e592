'use strict';

Object.defineProperty(exports, '__esModule', { value: true });

let parse = deps => Object.entries(deps || {}).map(([name, versionSpec]) => ({
  name,
  versionSpec
}));
let unparse = deps => {
  return Object.fromEntries(deps.map(({
    name,
    versionSpec
  }) => [name, versionSpec]));
};

exports.parse = parse;
exports.unparse = unparse;
